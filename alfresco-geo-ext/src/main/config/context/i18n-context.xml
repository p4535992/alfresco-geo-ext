<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans-2.0.dtd'>
<!-- i18n bean definitions -->
<beans>

    <bean id="execute-list-generation-action-messages"
          class="org.alfresco.i18n.ResourceBundleBootstrapComponent">
        <property name="resourceBundles">
            <list>
                <value>alfresco.module.${artifactId}.ui.geo-ext</value>
            </list>
        </property>
    </bean>

    <bean id="bootstrapRubisBundlesBean"
          class="org.alfresco.web.app.ResourceBundleBootstrap">
        <property name="resourceBundles">
            <list>
                <value>alfresco.module.${artifactId}.ui.geo-ext</value>
            </list>
        </property>
    </bean>

</beans>
